if( ( "HVACstC-3/2/54" and HZ_HVAC_FROST )!= 0u08)     then ModeWohnZ@ = CFROST@ endif;
if( ( "HVACstC-3/2/54" and (HZ_HVAC_FROST \\
                           or HZ_HVAC_NIGHT \\
                           or HZ_HVAC_KOMFORT \\
                           or HZ_HVAC_SBY)  )== 0u08)     then{
    ModeWohnZ@ = CERROR@ 
    }endif;

if( ( "HVACstE-3/2/56" and HZ_HVAC_SBY )!= 0u08)       then ModeEssZ@ = CSBY@ endif;
