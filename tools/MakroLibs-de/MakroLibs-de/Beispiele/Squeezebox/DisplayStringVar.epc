[Macros]
//Die Squeezebox wird eingebunden über den Standard Port 9090 und die IP-Adresse des Servers mit der Standardverzögerung von 100ms
Squeezebox(9090,192.168.22.18,100)
//Es wird eine Warnmeldung auf das Display geschickt, wenn die Temperatur unter 0 Grad fällt
DisplayStringVar(SQ1,Kalt,Außentemperatur%20unter%200Grad Frostgefahr!)
[MacroLibs]
EnertexSqueezebox.lib
[ETS-ESF]
// Die aus der ETS3 exportierte ESF-Datei
Haus.esf

[EibPC]
SQ1 = $Squeezebox$
Kalt = AUS
//Alle 5 Minuten wird überprüft, ob es draußen "Kalt" ist
If cycle(0,5) then If ("Außentemperatur-3/5/0" < 0f16) then Kalt = EIN endif endif